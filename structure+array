// Online C compiler to run C program online
#include <stdio.h>

int main() {
    struct student{
        int rollno;
        int marks;
        char grade;
        
    };
    struct student a[3];
    printf("Enter a details of student:\n");
    for(int i=0;i<3;i++)
    {
        printf("student%d\n",i+1);
         printf("roll no:");
        scanf("%d",&a[i].rollno);
         printf("marks:");
        scanf("%d",&a[i].marks);
         printf("grade:");
          getchar();
      scanf("%c",&a[i].grade);
        }
        printf("Rollno\tMarks\tGrade\n");
        for(int i=0;i<3;i++)
        {
            printf("%d\t\t%d\t\t%c\n",a[i].rollno,a[i].marks,a[i].grade);
         
        }
    
    

    return 0;
}
