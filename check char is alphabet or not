#include <stdio.h>

int main() {
    char ch;
    printf("enter a character :");
    scanf("%c",&ch);
    
   
     if ((ch>='a'&&ch<='z')||(ch>='A'&&ch<='Z'))
   
{
      
       printf("%c is character ",ch);
    
         
    }
 
 else 
 {
     printf("%c is not character ",ch);
 }
 
 
 
 
    return 0;
}
