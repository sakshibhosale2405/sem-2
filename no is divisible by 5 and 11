#include <stdio.h>

int main() {
    int num;
    printf("enter a no:");
    scanf("%d",&num);
    
   
    if ((num % 5==0)&&(num % 11==0))
{
      
       printf("no is divisible by 5 and 11");
    
         
    }
  else if (num%5==0)
 {
     printf("no is  divisible by 5 ");
 }
 else if (num%11==0)
 {
     printf("no is divisible by 11 ");
 }
 else
 {
     printf("no is not divisible by 5 and 11");
 }
 
 
 
 
    return 0;
}
