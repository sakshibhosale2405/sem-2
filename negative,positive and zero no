#include <stdio.h>

int main() {
    int a;
    printf("enter value of a: ");
    scanf("%d",&a);
    if(a==0)
    {
        printf("no is zero");
        
    }
    else if(a > 0)
    {
        printf("a is positive integer ");
    }
    else
    {
        printf("a is negative integer ");
    }
    return 0;
}
